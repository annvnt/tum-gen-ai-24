# API Configuration
OPENAI_API_KEY=your_openai_api_key_here
GCS_BUCKET_NAME=your_gcs_bucket_name
GCS_PROJECT_ID=your_gcs_project_id

# Port Configuration (Auto-resolved if conflicts)
API_PORT=8000
FRONTEND_PORT=3000
QDRANT_PORT=6333

# Backend Configuration
QDRANT_HOST=qdrant
NEXT_PUBLIC_API_URL=http://localhost:8000
NEXT_PUBLIC_WEBSOCKET_URL=ws://localhost:8000
BACKEND_URL=http://127.0.0.1:8000

# File Upload Configuration
NEXT_PUBLIC_MAX_FILE_SIZE=52428850
MAX_FILE_SIZE=52428850

# Security Configuration
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8000
CORS_ORIGINS=http://localhost:3000,http://localhost:8000

# Development Configuration
PYTHONPATH=/app
PYTHONUNBUFFERED=1
DEBUG=false
LOG_LEVEL=INFO

# Database Configuration
QDRANT_COLLECTION_NAME=financial_documents
QDRANT_VECTOR_SIZE=1536
QDRANT_DISTANCE=Cosine

# Google Cloud Storage
GOOGLE_APPLICATION_CREDENTIALS=/app/gcs-credentials.json
GCS_CREDENTIALS_PATH=/app/gcs-credentials.json
EOF < /dev/null